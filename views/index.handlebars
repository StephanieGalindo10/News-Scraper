<div class="articles container">


    <h1 class="card z-depth-3 center-align">Read Breaking News!</h1>
    <div class="row">
      <div class="col s3"></div>
      <h3 class="card z-depth-3 center-align col s6">Click to Scrape</h3>
    </div>
</div>


<div class="row">
  <div class="col s2"></div>


  <div class ="col s22 left-align">
    {{#each article}}


      <div class="card-panel">
        <h4>{{this.title}}</h4>


    
        <a href = "/readArticle/{{this._id}}" class="waves-effect waves-light btn green lighten-1" id="btnSignUp">Read Articles</a>
      </div>
    {{/each}}
  </div>
</div>